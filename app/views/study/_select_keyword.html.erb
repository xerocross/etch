<div>
<form class="" action="<%= revisit_flashcards_path %>" method="GET">

    <select class = "form-control" name="keyword">
    <% keywords.each do |keyword| %>
        <%="<option value=\"#{keyword.id}\">#{keyword.text}</option>".html_safe %>
    <% end %>
    </select>
    <input class="form-control btn-group btn btn-default" type="submit" value="go">
</form>
</div>